
{{define "pagelayout"}}
<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />

    <!-- Bootstrap Min CSS -->
    <link rel="stylesheet" href="static/css/bootstrap.min.css" />
    <!-- Animate Min CSS -->
    <link rel="stylesheet" href="static/css/animate.min.css" />
    <!-- FlatIcon CSS -->
    <link rel="stylesheet" href="static/css/flaticon.css" />
    <!-- FlatIcon Two CSS -->
    <link rel="stylesheet" href="static/css/flaticon-two.css" />
    <!-- FlatIcon Three CSS -->
    <link rel="stylesheet" href="static/css/flaticon-three.css" />
    <!-- Odometer Min CSS -->
    <link rel="stylesheet" href="static/css/odometer.min.css" />
    <!-- MeanMenu CSS -->
    <link rel="stylesheet" href="static/css/meanmenu.css" />
    <!-- Magnific Popup Min CSS -->
    <link rel="stylesheet" href="static/css/magnific-popup.min.css" />
    <!-- Nice Select Min CSS -->
    <link rel="stylesheet" href="static/css/nice-select.min.css" />
    <!-- Owl Carousel Min CSS -->
    <link rel="stylesheet" href="static/css/owl.carousel.min.css" />
    <!-- Font Awesome Min CSS -->
    <link rel="stylesheet" href="static/css/fontawesome.min.css" />
    <!-- Style CSS -->
    <link rel="stylesheet" href="static/css/style.css" />
    <!-- Responsive CSS -->
    <link rel="stylesheet" href="static/css/responsive.css" />

    <title>
      {{template "title" .}} - Школа иностранных языков JOY
    </title>

    <link rel="icon" type="image/png" href="static/img/favicon.png" />
  </head>

  <body>
    <!-- Preloader Area -->
    <div class="preloader-area">
      <div class="spinner">
        <div class="dot1"></div>
        <div class="dot2"></div>
      </div>
    </div>
    <!-- End Preloader Area -->

    <!-- Start Navbar Area Two -->
    <div class="navbar-area-two">
      <div class="exto-responsive-nav">
        <div class="container">
          <div class="exto-responsive-menu">
            <div class="logo">
              <a href="/">
                <img
                  src="static/img/logo/logo.png"
                  alt="Школа иностранных языков JOY"
                />
              </a>
            </div>
          </div>
        </div>
      </div>

      <div class="exto-nav">
        <div class="container">
          <nav class="navbar navbar-expand-md navbar-light">
            <a class="navbar-brand" href="/">
              <img
                src="static/img/logo/logo.png"
                alt="Школа иностранных языков JOY"
              />
            </a>
            <div
              class="collapse navbar-collapse mean-menu"
              id="navbarSupportedContent"
            >
              <ul class="navbar-nav">
                <li class="nav-item">
                  <a href="#нас" class="nav-link">О нас</a>
                </li>
                <li class="nav-item">
                  <a href="#программы" class="nav-link"> Программы </a>
                </li>
                <li class="nav-item">
                  <a href="#преподаватели" class="nav-link"> Преподаватели </a>
                </li>
                <li class="nav-item">
                  <a href="#новости" class="nav-link"> Новости </a>
                </li>
                <li class="nav-item">
                  <a href="#отзывы" class="nav-link"> Отзывы </a>
                </li>
                <li class="nav-item">
                  <a href="#контакты " class="nav-link"> Контакты </a>
                </li>
              </ul>

              <div class="others-options seo-option">
                <div class="phone-us">
                  <img src="static/img/seo/call.png" alt="" />
                  <span class="nav-link">
                    +79535519099 <br />
                    +79200601660
                  </span>
                </div>
              </div>
            </div>
          </nav>
        </div>
      </div>
    </div>
    <!-- End Navbar Area Two -->

    {{template "pageheader" .}}

    {{template "pagebody" .}}

    {{template "footer" .}}

    <!-- Start Go Top Section -->
    <div class="go-top">
      <i class="fas fa-chevron-up"></i>
      <i class="fas fa-chevron-up"></i>
    </div>
    <!-- End Go Top Section -->

    <!-- jQuery Min JS -->
    <script src="static/js/jquery.min.js"></script>
    <!-- Popper Min JS -->
    <script src="static/js/popper.min.js"></script>
    <!-- Bootstrap Min JS -->
    <script src="static/js/bootstrap.min.js"></script>
    <!-- MeanMenu JS  -->
    <script src="static/js/jquery.meanmenu.js"></script>
    <!-- Appear Min JS -->
    <script src="static/js/jquery.appear.min.js"></script>
    <!-- Odometer Min JS -->
    <script src="static/js/odometer.min.js"></script>
    <!-- Owl Carousel Min JS -->
    <script src="static/js/owl.carousel.min.js"></script>
    <!-- Magnific Popup Min JS -->
    <script src="static/js/jquery.magnific-popup.min.js"></script>
    <!-- Nice Select Min JS -->
    <script src="static/js/jquery.nice-select.min.js"></script>
    <!-- WOW Min JS -->
    <script src="static/js/wow.min.js"></script>
    <!-- Form Validator JS -->
    <script src="static/js/form-validator.min.js"></script>
    <!-- Ajaxchimp Min JS -->
    <script src="static/js/jquery.ajaxchimp.min.js"></script>
    <!-- Contact JS -->
    <script src="static/js/contact-form-script.js"></script>
    <!-- Particles JS -->
    <script src="static/js/particles.min.js"></script>
    <!-- Progressbar Min JS -->
    <script src="static/js/progressbar.min.js"></script>
    <!-- Main JS -->
    <script src="static/js/main.js"></script>
